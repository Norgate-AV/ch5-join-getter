        "husky": "^9.0.10",
        "lint-staged": "^15.2.2",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "semantic-release": "^23.0.2",
        "terser": "^5.27.0",
        "tsup": "^8.0.1",
        "type-fest": "^4.10.2",
        "typescript": "^5.3.3",
        "vitest": "^1.2.2"
